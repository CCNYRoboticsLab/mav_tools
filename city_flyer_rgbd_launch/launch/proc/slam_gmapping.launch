<launch>
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" 
      output="log">

      <remap from="/scan" to="/scan_virtual_middle"/>

      <param name="odom_frame" value="odom"/>
      <param name="base_frame" value="base_ortho"/>

      <param name="map_udpate_interval" value="1.0"/>

      <param name="particles" value="5"/>
      <param name="delta" value="0.10"/>
      <param name="maxUrange" value="40.0"/>
      <param name="lskip" value="1"/>

      <param name="sigma" value="0.05"/>
      <param name="kernelSize" value="1"/>
      <param name="lstep" value="0.2"/>
      <param name="astep" value="0.2"/>

      <param name="iterations" value="5"/>

      <param name="lsigma" value="0.075"/>
      <param name="ogain" value="3.0"/>
      <param name="srr" value="0.001"/>
      <param name="srt" value="0.002"/>
      <param name="str" value="0.001"/>
      <param name="stt" value="0.010"/>

      <param name="linearUpdate" value="0.1"/>
      <param name="angularUpdate" value="0.1"/>
      <param name="temporalUpdate" value="0.1"/>
      <param name="resampleThreshold" value="0.5"/>

      <param name="xmin" value="-10.0"/>
      <param name="ymin" value="-10.0"/>
      <param name="xmax" value="10.0"/>
      <param name="ymax" value="10.0"/>

      <param name="llsamplerange" value="0.01"/>
      <param name="llsamplestep" value="0.01"/>
      <param name="lasamplerange" value="0.005"/>
      <param name="lasamplestep" value="0.005"/>

    </node>
</launch>
